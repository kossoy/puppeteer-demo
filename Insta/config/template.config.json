{
  "baseUrl": "https://www.instagram.com",
  "username": "username",
  "password": "password",

  "hashTags": [
    "vocalists",
    "popmusic"
  ],

  "settings": {
    "headless": true,
    "verbose": true,
    "viewport": {
      "width": 1000,
      "height": 1080
    },
    "args": [
      "--no-sandbox"
    ],
    "resourcePath": "resources"
  },

  "selectors": {
    "username": "input[name=\"username\"]",
    "password": "input[name=\"password\"]",
    "loginButton": "button[type=\"submit\"]",
    "followers": "div > section > main > div > header > section > ul > li:nth-child(2) > a",
    "notNowButton": ".HoLwm"
  }
}
